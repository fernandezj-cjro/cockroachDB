    "import os\n",
    "import psycopg2\n",
    "\n",
    "conn = psycopg2.connect(os.environ[\"DATABASE_URL\"])\n",
    "\n",
    "with conn.cursor() as cur:\n",
    "    cur.execute(\"SELECT now()\")\n",
    "    res = cur.fetchall()\n",
    "    conn.commit()\n",
    "    print(res)"
